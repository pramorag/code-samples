<?xml version="1.0"?>
<Cisco-IOS-XR-config-cfgmgr-cfg ns="http://cisco.com/ns/yang/Cisco-IOS-XR-config-cfgmgr-cfg" revision="2015-11-09">
  <files>
    <file revision="2015-11-09" submodule="false">Cisco-IOS-XR-config-cfgmgr-cfg.yang</file>
  </files>
  <prefix>config-cfgmgr-cfg</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <import prefix="xr">Cisco-IOS-XR-types</import>
  <cfgmgr config="true" is-key="false" node="container" presence="false">
    <xr-path>/cfg/gl/cfgmgr/</xr-path>
    <xr-xml-map>cfgmgr_cfg:Cfgmgr</xr-xml-map>
    <xr-scope>exported</xr-scope>
    <mode-exclusive config="true" is-key="false" is-yang-key="false" node="leaf">
      <xr-primitive-type>uint32</xr-primitive-type>
      <xr-path>/cfg/gl/cfgmgr/exclusive</xr-path>
      <xr-xml-map>cfgmgr_cfg:ModeExclusive</xr-xml-map>
      <xr-sch-type>Boolean</xr-sch-type>
      <type>
        <type-name>boolean</type-name>
      </type>
      <default>true</default>
    </mode-exclusive>
  </cfgmgr>
</Cisco-IOS-XR-config-cfgmgr-cfg>

<?xml version="1.0"?>
<Cisco-IOS-XR-config-mibs-cfg ns="http://cisco.com/ns/yang/Cisco-IOS-XR-config-mibs-cfg" revision="2015-09-29">
  <files>
    <file revision="2015-09-29" submodule="false">Cisco-IOS-XR-config-mibs-cfg.yang</file>
  </files>
  <prefix>config-mibs-cfg</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <import prefix="xr">Cisco-IOS-XR-types</import>
  <import prefix="a1">Cisco-IOS-XR-snmp-agent-cfg</import>
  <augment path="/a1:snmp/a1:notification">
    <config-man config="true" is-key="false" node="container" presence="false">
      <xr-path>/cfg/gl/snmp/enable/traps/config/</xr-path>
      <xr-xml-map>configmanmib_cfg:ConfigMan</xr-xml-map>
      <xr-task>snmp</xr-task>
      <enable config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>uint32</xr-primitive-type>
        <xr-path>/cfg/gl/snmp/enable/traps/config/enable</xr-path>
        <xr-xml-map>configmanmib_cfg:Enable</xr-xml-map>
        <xr-task>snmp</xr-task>
        <xr-sch-type>Configured</xr-sch-type>
        <type>
          <type-name>empty</type-name>
        </type>
      </enable>
    </config-man>
  </augment>
</Cisco-IOS-XR-config-mibs-cfg>

<?xml version="1.0"?>
<Cisco-IOS-XR-infra-rsi-subscriber-cfg ns="http://cisco.com/ns/yang/Cisco-IOS-XR-infra-rsi-subscriber-cfg" revision="2015-07-30">
  <files>
    <file revision="2015-07-30" submodule="false">Cisco-IOS-XR-infra-rsi-subscriber-cfg.yang</file>
  </files>
  <prefix>infra-rsi-subscriber-cfg</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <import prefix="xr">Cisco-IOS-XR-types</import>
  <import prefix="a1">Cisco-IOS-XR-subscriber-infra-tmplmgr-cfg</import>
  <augment path="/a1:dynamic-template/a1:subscriber-services/a1:subscriber-service">
    <vrf config="true" is-key="false" is-yang-key="false" node="leaf">
      <xr-primitive-type>string</xr-primitive-type>
      <xr-path>/cfg/gl/dynamic-templates/service/&lt;subscriber_infra_subdb_cfg_SubscriberService_name&gt;/rsi/vrf</xr-path>
      <xr-xml-map>cfg_vrf_dyn_tmpl:VRF</xr-xml-map>
      <xr-task>network</xr-task>
      <xr-sch-type>BoundedString</xr-sch-type>
      <type length="1..32">
        <type-name>xr:Cisco-ios-xr-string</type-name>
      </type>
    </vrf>
  </augment>
  <augment path="/a1:dynamic-template/a1:ppps/a1:ppp">

<?xml version="1.0"?>
<Cisco-IOS-XR-drivers-media-eth-act ns="http://cisco.com/ns/yang/Cisco-IOS-XR-drivers-media-eth-act" revision="2018-02-12">
  <files>
    <file revision="2018-02-12" submodule="false">Cisco-IOS-XR-drivers-media-eth-act.yang</file>
  </files>
  <prefix>eth-drvr-act</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <import prefix="csc">Cisco-IOS-XR-types</import>
  <clear-controller-counters config="true" is-key="false" node="rpc" presence="false">
    <xr-task>interface</xr-task>
    <xr-cli-command>clear controller &lt;controller-name&gt; stats</xr-cli-command>
    <output config="false" is-key="false" node="output"/>
    <input config="false" is-key="false" node="input">
      <controller-name config="false" is-key="false" is-yang-key="false" node="leaf">
        <xr-task>interface</xr-task>
        <mandatory>true</mandatory>
        <type>
          <type-name>csc:Interface-name</type-name>
        </type>
      </controller-name>
    </input>
  </clear-controller-counters>
</Cisco-IOS-XR-drivers-media-eth-act>

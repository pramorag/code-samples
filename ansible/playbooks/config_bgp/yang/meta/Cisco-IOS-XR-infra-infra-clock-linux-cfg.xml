<?xml version="1.0"?>
<Cisco-IOS-XR-infra-infra-clock-linux-cfg ns="http://cisco.com/ns/yang/Cisco-IOS-XR-infra-infra-clock-linux-cfg" revision="2015-11-09">
  <files>
    <file revision="2015-11-09" submodule="false">Cisco-IOS-XR-infra-infra-clock-linux-cfg.yang</file>
  </files>
  <prefix>infra-infra-clock-linux-cfg</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <import prefix="xr">Cisco-IOS-XR-types</import>
  <clock config="true" is-key="false" node="container" presence="false">
    <xr-path>/cfg/gl/clock/</xr-path>
    <xr-xml-map>clock_cfg_linux:Clock</xr-xml-map>
    <xr-task>host-services</xr-task>
    <time-zone config="true" is-key="false" node="container" presence="true">
      <xr-data-handler>ignore</xr-data-handler>
      <xr-is-packed/>
      <xr-path>/cfg/gl/clock/tz1`</xr-path>
      <xr-xml-map>clock_cfg_linux:TimeZone</xr-xml-map>
      <xr-task>host-services</xr-task>
      <xr-pack-format>%s%s</xr-pack-format>
      <time-zone-name config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-is-packed/>
        <xr-pack-format>%s%s</xr-pack-format>
        <xr-pack-elem-size>1</xr-pack-elem-size>
        <xr-path>/cfg/gl/clock/tz1`0</xr-path>
        <xr-xml-map>clock_cfg_linux:TimeZoneName</xr-xml-map>
        <xr-task>host-services</xr-task>
        <xr-sch-type>String</xr-sch-type>
        <type>
          <type-name>string</type-name>
        </type>
        <mandatory>true</mandatory>
      </time-zone-name>
      <area-name config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-is-packed/>
        <xr-pack-format>%s%s</xr-pack-format>
        <xr-pack-elem-size>1</xr-pack-elem-size>
        <xr-path>/cfg/gl/clock/tz1`1</xr-path>
        <xr-xml-map>clock_cfg_linux:AreaName</xr-xml-map>
        <xr-task>host-services</xr-task>
        <xr-sch-type>String</xr-sch-type>
        <type>
          <type-name>string</type-name>
        </type>
        <mandatory>true</mandatory>
      </area-name>
    </time-zone>
  </clock>
</Cisco-IOS-XR-infra-infra-clock-linux-cfg>

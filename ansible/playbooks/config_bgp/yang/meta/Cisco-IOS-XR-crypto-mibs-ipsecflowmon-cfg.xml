<?xml version="1.0"?>
<Cisco-IOS-XR-crypto-mibs-ipsecflowmon-cfg ns="http://cisco.com/ns/yang/Cisco-IOS-XR-crypto-mibs-ipsecflowmon-cfg" revision="2015-11-09">
  <files>
    <file revision="2015-11-09" submodule="false">Cisco-IOS-XR-crypto-mibs-ipsecflowmon-cfg.yang</file>
  </files>
  <prefix>crypto-mibs-ipsecflowmon-cfg</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <import prefix="xr">Cisco-IOS-XR-types</import>
  <import prefix="a1">Cisco-IOS-XR-snmp-agent-cfg</import>
  <augment path="/a1:snmp/a1:notification">
    <ip-sec config="true" is-key="false" node="container" presence="false">
      <xr-path>/cfg/gl/snmp/enable/traps/ipsec/</xr-path>
      <xr-xml-map>ipsecflowmonmib_cfg:IPSec</xr-xml-map>
      <xr-task>crypto</xr-task>
      <tunnel-stop config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>uint32</xr-primitive-type>
        <xr-path>/cfg/gl/snmp/enable/traps/ipsec/tunnel/stop</xr-path>
        <xr-xml-map>ipsecflowmonmib_cfg:TunnelStop</xr-xml-map>
        <xr-task>crypto</xr-task>
        <xr-sch-type>Configured</xr-sch-type>
        <type>
          <type-name>empty</type-name>
        </type>
      </tunnel-stop>
      <tunnel-start config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>uint32</xr-primitive-type>
        <xr-path>/cfg/gl/snmp/enable/traps/ipsec/tunnel/start</xr-path>
        <xr-xml-map>ipsecflowmonmib_cfg:TunnelStart</xr-xml-map>
        <xr-task>crypto</xr-task>
        <xr-sch-type>Configured</xr-sch-type>
        <type>
          <type-name>empty</type-name>
        </type>
      </tunnel-start>
    </ip-sec>
  </augment>
  <augment path="/a1:snmp/a1:notification">
    <isakmp config="true" is-key="false" node="container" presence="false">
      <xr-path>/cfg/gl/snmp/enable/traps/isakmp/</xr-path>
      <xr-xml-map>ipsecflowmonmib_cfg:ISAKMP</xr-xml-map>
      <xr-task>crypto</xr-task>
      <tunnel-stop config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>uint32</xr-primitive-type>
        <xr-path>/cfg/gl/snmp/enable/traps/isakmp/tunnel/stop</xr-path>
        <xr-xml-map>ipsecflowmonmib_cfg:TunnelStop</xr-xml-map>
        <xr-task>crypto</xr-task>
        <xr-sch-type>Configured</xr-sch-type>
        <type>
          <type-name>empty</type-name>
        </type>
      </tunnel-stop>
      <tunnel-start config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>uint32</xr-primitive-type>
        <xr-path>/cfg/gl/snmp/enable/traps/isakmp/tunnel/start</xr-path>
        <xr-xml-map>ipsecflowmonmib_cfg:TunnelStart</xr-xml-map>
        <xr-task>crypto</xr-task>
        <xr-sch-type>Configured</xr-sch-type>
        <type>
          <type-name>empty</type-name>
        </type>
      </tunnel-start>
    </isakmp>
  </augment>
</Cisco-IOS-XR-crypto-mibs-ipsecflowmon-cfg>

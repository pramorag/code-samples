<?xml version="1.0"?>
<Cisco-IOS-XR-types ns="http://cisco.com/ns/yang/cisco-xr-types" revision="2018-06-29">
  <files>
    <file revision="2018-06-29" submodule="false">Cisco-IOS-XR-types.yang</file>
  </files>
  <prefix>xr</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <typedef name="Route-dist">
    <type>
      <pattern>[a-fA-F0-9]{16}</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Bgp-l2vpn-evpn-addrs">
    <type>
      <pattern>[a-fA-F0-9]{58}</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Bgp-ls-addr">
    <type>
      <pattern>[a-fA-F0-9]+</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Bgp-ipv6-mvpn-addr">
    <type>
      <pattern>[a-fA-F0-9]{104}</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Bgp-ipv4-mvpn-addr">
    <type>
      <pattern>[a-fA-F0-9]{56}</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Bgp-rt-constrt-addr">
    <type>
      <pattern>[a-fA-F0-9]{24}</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Bgp-ipv4-mdt-addr">
    <type>
      <pattern>(([a-f0-9]{16}-)(([1-9]?[0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])\.){3}([1-9]?[0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5]))</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Bgp-ipv4-tunnel-addr">
    <type>
      <pattern>((0:|[1-9][0-9]{0,4}:)(([1-9]?[0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])\.){3}([1-9]?[0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5]))</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Cisco-ios-xr-port-number">
    <type range="1..65535">
      <type-name>uint16</type-name>
    </type>
  </typedef>
  <typedef name="Interface-name">
    <type>
      <pattern>[a-zA-Z0-9._/-]+</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Cisco-ios-xr-string">
    <type>
      <pattern>[\w\-\.:,_@#%$\+=\|;]+</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Ipv4-prefix-length">
    <type range="0..32">
      <type-name>uint8</type-name>
    </type>
  </typedef>
  <typedef name="Ipv6-prefix-length">
    <type range="0..128">
      <type-name>uint8</type-name>
    </type>
  </typedef>
  <typedef name="Rack-id">
    <type>
      <pattern>[a-zA-Z0-9_]*\d+</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Slot-id">
    <type>
      <pattern>[a-zA-Z0-9_]*\d+</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Instance-id">
    <type>
      <pattern>[a-zA-Z0-9_]*\d+</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Sub-instance-id">
    <type>
      <pattern>[a-zA-Z0-9_]*\d+</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Encryption-type">
    <type node="enumeration">
      <enum value="0">none</enum>
      <enum value="1">md5</enum>
      <enum value="2">proprietary</enum>
      <enum value="3">type6</enum>
    </type>
  </typedef>
  <typedef name="Hex-integer">
    <type>
      <pattern>[0-9a-fA-F]{1,8}</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Hex-integer-16">
    <type>
      <pattern>[0-9a-fA-F]{1,4}</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Osi-system-id">
    <type>
      <pattern>[a-fA-F0-9]{4}(\.[a-fA-F0-9]{4}){2}</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Osi-area-address">
    <type>
      <pattern>[a-fA-F0-9]{2}(\.[a-fA-F0-9]{4}){0,6}</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Isis-node-id">
    <type>
      <pattern>[a-fA-F0-9]{4}(\.[a-fA-F0-9]{4}){2}\.[a-fA-F0-9]{2}</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Isis-snpa">
    <type>
      <pattern>[a-fA-F0-9]{4}(\.[a-fA-F0-9]{4}){2}</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Isis-lsp-id">
    <type>
      <pattern>[a-fA-F0-9]{4}(\.[a-fA-F0-9]{4}){2}\.[a-fA-F0-9]{2}\-[a-fA-F0-9]{2}</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Osi-net">
    <type>
      <pattern>[a-fA-F0-9]{2}(\.[a-fA-F0-9]{4}){3,9}\.[a-fA-F0-9]{2}</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="String-identifier">
    <type>
      <pattern>[a-zA-Z][\w\-]*</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Char-num">
    <type node="union">
      <type>
        <pattern>(\p{IsBasicLatin}|\p{IsLatin-1Supplement})*</pattern>
        <type-name>string</type-name>
      </type>
      <type>
        <type-name>uint8</type-name>
      </type>
    </type>
  </typedef>
  <typedef name="Tty-escape-char-num">
    <type node="union">
      <type>
        <pattern>(\p{IsBasicLatin}|\p{IsLatin-1Supplement})|(DEFAULT)|(BREAK)|(NONE)</pattern>
        <type-name>string</type-name>
      </type>
      <type>
        <type-name>uint8</type-name>
      </type>
    </type>
  </typedef>
  <typedef name="Extended-node-id">
    <type>
      <pattern>([a-zA-Z0-9_]*\d+/){3}([a-zA-Z0-9_]*\d+)</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Node-id">
    <type>
      <pattern>([a-zA-Z0-9_]*\d+/){1,2}([a-zA-Z0-9_]*\d+)</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Pq-node-id">
    <type>
      <pattern>((([a-zA-Z0-9_]*\d+)|(\*))/){2}(([a-zA-Z0-9_]*\d+)|(\*))</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Md5-password">
    <type>
      <pattern>(!.+)|([^!].+)</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Type8-password">
    <type>
      <pattern>(!.+)|([^!].+)</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Type9-password">
    <type>
      <pattern>(!.+)|([^!].+)</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Proprietary-password">
    <type>
      <pattern>(!.+)|([^!].+)</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Type6-password">
    <type>
      <pattern>(!.+)|([^!].+)</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Bgp-ipv4-flowspec-address">
    <type>
      <pattern>[a-fA-F0-9]{4096}</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Bgp-ipv6-flowspec-address">
    <type>
      <pattern>[a-fA-F0-9]{4096}</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Rpl-policy">
    <type>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Rpl-set">
    <type>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="Physical-allowed-node-id">
    <type>
      <type-name>uint32</type-name>
    </type>
  </typedef>
  <typedef name="Physical-allowed-node-id-string">
    <type>
      <type-name>string</type-name>
    </type>
  </typedef>
</Cisco-IOS-XR-types>

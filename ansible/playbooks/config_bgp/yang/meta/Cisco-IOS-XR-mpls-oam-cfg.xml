<?xml version="1.0"?>
<Cisco-IOS-XR-mpls-oam-cfg ns="http://cisco.com/ns/yang/Cisco-IOS-XR-mpls-oam-cfg" revision="2015-11-09">
  <files>
    <file revision="2015-11-09" submodule="false">Cisco-IOS-XR-mpls-oam-cfg.yang</file>
  </files>
  <prefix>mpls-oam-cfg</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <import prefix="xr">Cisco-IOS-XR-types</import>
  <mpls-oam config="true" is-key="false" node="container" presence="false">
    <xr-path>/cfg/gl/mpls_lspv/</xr-path>
    <xr-xml-map>mpls_oam_cfg:MPLS_OAM</xr-xml-map>
    <xr-task>mpls-te | mpls-ldp | mpls-static</xr-task>
    <xr-scope>exported</xr-scope>
    <reply-mode config="true" is-key="false" node="container" presence="false">
      <xr-path>/cfg/gl/mpls_lspv/reply_mode/</xr-path>
      <xr-xml-map>mpls_oam_cfg:ReplyMode</xr-xml-map>
      <xr-task>mpls-te | mpls-ldp | mpls-static</xr-task>
      <control-channel config="true" is-key="false" node="container" presence="false">
        <xr-path>/cfg/gl/mpls_lspv/reply_mode/control_channel/</xr-path>
        <xr-xml-map>mpls_oam_cfg:ControlChannel</xr-xml-map>
        <xr-task>mpls-te | mpls-ldp | mpls-static</xr-task>
        <allow-reverse-lsp config="true" is-key="false" is-yang-key="false" node="leaf">
          <xr-primitive-type>uint32</xr-primitive-type>
          <xr-path>/cfg/gl/mpls_lspv/reply_mode/control_channel/allow_reverse_lsp</xr-path>
          <xr-xml-map>mpls_oam_cfg:AllowReverseLSP</xr-xml-map>
          <xr-task>mpls-te | mpls-ldp | mpls-static</xr-task>
          <xr-sch-type>Configured</xr-sch-type>
          <type>
            <type-name>empty</type-name>
          </type>
        </allow-reverse-lsp>
      </control-channel>
    </reply-mode>
    <enable-oam config="true" is-key="false" is-yang-key="false" node="leaf">
      <xr-primitive-type>uint32</xr-primitive-type>
      <xr-path>/cfg/gl/mpls_lspv/enable</xr-path>
      <xr-xml-map>mpls_oam_cfg:EnableOAM</xr-xml-map>
      <xr-task>mpls-te | mpls-ldp | mpls-static</xr-task>
      <xr-sch-type>Configured</xr-sch-type>
      <type>
        <type-name>empty</type-name>
      </type>
    </enable-oam>
    <disable-vendor-extension config="true" is-key="false" is-yang-key="false" node="leaf">
      <xr-primitive-type>uint32</xr-primitive-type>
      <xr-path>/cfg/gl/mpls_lspv/disable_vendor_ext</xr-path>
      <xr-xml-map>mpls_oam_cfg:DisableVendorExtension</xr-xml-map>
      <xr-task>mpls-te | mpls-ldp | mpls-static</xr-task>
      <xr-sch-type>Configured</xr-sch-type>
      <type>
        <type-name>empty</type-name>
      </type>
    </disable-vendor-extension>
  </mpls-oam>
</Cisco-IOS-XR-mpls-oam-cfg>

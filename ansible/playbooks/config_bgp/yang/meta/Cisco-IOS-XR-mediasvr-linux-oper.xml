<?xml version="1.0"?>
<Cisco-IOS-XR-mediasvr-linux-oper ns="http://cisco.com/ns/yang/Cisco-IOS-XR-mediasvr-linux-oper" revision="2015-11-09">
  <files>
    <file revision="2015-11-09" submodule="false">Cisco-IOS-XR-mediasvr-linux-oper.yang</file>
    <file revision="2015-11-09" submodule="true">Cisco-IOS-XR-mediasvr-linux-oper-sub1.yang</file>
  </files>
  <prefix>mediasvr-linux-oper</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <import prefix="xr">Cisco-IOS-XR-types</import>
  <media-svr config="false" is-key="false" node="container" presence="false">
    <xr-path>/oper/mediasvr/gl/</xr-path>
    <xr-xml-map>show_mediang_oper:MediaSvr</xr-xml-map>
    <xr-task>basic-services</xr-task>
    <all config="false" is-key="false" node="container" presence="false">
      <xr-path>/oper/mediasvr/gl/all</xr-path>
      <xr-xml-map>show_mediang_oper:All</xr-xml-map>
      <xr-task>basic-services</xr-task>
      <xr-bag-grp-name>show_media_edm_string</xr-bag-grp-name>
      <xr-bag>showmedia.bag</xr-bag>
      <show-output config="false" is-key="false" is-yang-key="false" node="leaf">
        <xr-path>/oper/mediasvr/gl/all~show_media_edm_string.show_output</xr-path>
        <xr-xml-map>showmedia:show_output</xr-xml-map>
        <xr-task>basic-services</xr-task>
        <xr-bag-primitive-type>string</xr-bag-primitive-type>
        <xr-bag-version>1 0</xr-bag-version>
        <type>
          <type-name>string</type-name>
        </type>
      </show-output>
    </all>
    <location-descriptions config="false" is-key="false" node="container" presence="false">
      <xr-path>/oper/mediasvr/gl/show_media/</xr-path>
      <xr-xml-map>show_mediang_oper:ShowMedia</xr-xml-map>
      <xr-task>basic-services</xr-task>
      <location-description config="false" key="node" node="list">
        <xr-path>/oper/mediasvr/gl/show_media/&lt;show_mediang_oper_LocationDescription_location&gt;</xr-path>
        <xr-xml-map>show_mediang_oper:LocationDescription</xr-xml-map>
        <xr-task>basic-services</xr-task>
        <xr-bag-grp-name>show_media_edm_string</xr-bag-grp-name>
        <xr-bag>showmedia.bag</xr-bag>
        <node config="false" is-key="true" is-yang-key="true" node="leaf">
          <xr-data-handler>ignore</xr-data-handler>
          <xr-pathformat>hex_nozerox</xr-pathformat>
          <xr-primitive-type>uint32</xr-primitive-type>
          <xr-path>/oper/mediasvr/gl/show_media/&lt;show_mediang_oper_LocationDescription_location&gt;</xr-path>
          <xr-xml-map>show_mediang_oper:Node</xr-xml-map>
          <xr-task>basic-services</xr-task>
          <xr-sch-type>NodeID</xr-sch-type>
          <xr-sch-arg-name>&lt;show_mediang_oper_LocationDescription_location&gt;</xr-sch-arg-name>
          <type>
            <type-name>xr:Node-id</type-name>
          </type>
        </node>
        <show-output config="false" is-key="false" is-yang-key="false" node="leaf">
          <xr-path>/oper/mediasvr/gl/show_media/&lt;show_mediang_oper_LocationDescription_location&gt;~show_media_edm_string.show_output</xr-path>
          <xr-xml-map>showmedia:show_output</xr-xml-map>
          <xr-task>basic-services</xr-task>
          <xr-bag-primitive-type>string</xr-bag-primitive-type>
          <xr-bag-version>1 0</xr-bag-version>
          <type>
            <type-name>string</type-name>
          </type>
        </show-output>
      </location-description>
    </location-descriptions>
  </media-svr>
</Cisco-IOS-XR-mediasvr-linux-oper>

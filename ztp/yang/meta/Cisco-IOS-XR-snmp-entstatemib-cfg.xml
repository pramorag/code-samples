<?xml version="1.0"?>
<Cisco-IOS-XR-snmp-entstatemib-cfg ns="http://cisco.com/ns/yang/Cisco-IOS-XR-snmp-entstatemib-cfg" revision="2015-07-27">
  <files>
    <file revision="2015-07-27" submodule="false">Cisco-IOS-XR-snmp-entstatemib-cfg.yang</file>
  </files>
  <prefix>snmp-entstatemib-cfg</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <import prefix="xr">Cisco-IOS-XR-types</import>
  <import prefix="a1">Cisco-IOS-XR-snmp-agent-cfg</import>
  <augment path="/a1:snmp/a1:notification">
    <entity-state config="true" is-key="false" node="container" presence="false">
      <xr-path>/cfg/gl/snmp/enable/traps/entity-state/</xr-path>
      <xr-xml-map>entitystatemib_cfg:EntityState</xr-xml-map>
      <xr-task>snmp</xr-task>
      <switchover config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>uint32</xr-primitive-type>
        <xr-path>/cfg/gl/snmp/enable/traps/entity-state/SwitchoverNotif</xr-path>
        <xr-xml-map>entitystatemib_cfg:Switchover</xr-xml-map>
        <xr-task>snmp</xr-task>
        <xr-sch-type>Configured</xr-sch-type>
        <type>
          <type-name>empty</type-name>
        </type>
      </switchover>
      <oper-status config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>uint32</xr-primitive-type>
        <xr-path>/cfg/gl/snmp/enable/traps/entity-state/OperNotif</xr-path>
        <xr-xml-map>entitystatemib_cfg:OperStatus</xr-xml-map>
        <xr-task>snmp</xr-task>
        <xr-sch-type>Configured</xr-sch-type>
        <type>
          <type-name>empty</type-name>
        </type>
      </oper-status>
    </entity-state>
  </augment>
</Cisco-IOS-XR-snmp-entstatemib-cfg>

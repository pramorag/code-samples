<?xml version="1.0"?>
<Cisco-IOS-XR-snmp-bridgemib-cfg ns="http://cisco.com/ns/yang/Cisco-IOS-XR-snmp-bridgemib-cfg" revision="2015-11-09">
  <files>
    <file revision="2015-11-09" submodule="false">Cisco-IOS-XR-snmp-bridgemib-cfg.yang</file>
  </files>
  <prefix>snmp-bridgemib-cfg</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <import prefix="xr">Cisco-IOS-XR-types</import>
  <import prefix="a1">Cisco-IOS-XR-snmp-agent-cfg</import>
  <augment path="/a1:snmp/a1:notification">
    <bridge config="true" is-key="false" node="container" presence="false">
      <xr-path>/cfg/gl/snmp/enable/traps/bridgemib/</xr-path>
      <xr-xml-map>bridgemib_cfg:Bridge</xr-xml-map>
      <xr-task>snmp</xr-task>
      <enable config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>uint32</xr-primitive-type>
        <xr-path>/cfg/gl/snmp/enable/traps/bridgemib/enable</xr-path>
        <xr-xml-map>bridgemib_cfg:Enable</xr-xml-map>
        <xr-task>snmp</xr-task>
        <xr-sch-type>Configured</xr-sch-type>
        <type>
          <type-name>empty</type-name>
        </type>
      </enable>
    </bridge>
  </augment>
</Cisco-IOS-XR-snmp-bridgemib-cfg>

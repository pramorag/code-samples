        <xr-primitive-type>uint32</xr-primitive-type>
        <xr-path>/cfg/gl/snmp/enable/traps/diameter/TransientFailNotif</xr-path>
        <xr-xml-map>aaa_diameter_base_mib:Transientfail</xr-xml-map>

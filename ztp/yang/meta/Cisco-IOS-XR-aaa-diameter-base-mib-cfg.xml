<?xml version="1.0"?>
<Cisco-IOS-XR-aaa-diameter-base-mib-cfg ns="http://cisco.com/ns/yang/Cisco-IOS-XR-aaa-diameter-base-mib-cfg" revision="2015-11-09">
  <files>
    <file revision="2015-11-09" submodule="false">Cisco-IOS-XR-aaa-diameter-base-mib-cfg.yang</file>
  </files>
  <prefix>aaa-diameter-base-mib-cfg</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <import prefix="xr">Cisco-IOS-XR-types</import>
  <import prefix="a1">Cisco-IOS-XR-snmp-agent-cfg</import>
  <augment path="/a1:snmp/a1:notification">
    <diametermib config="true" is-key="false" node="container" presence="false">
      <xr-path>/cfg/gl/snmp/enable/traps/diameter/</xr-path>
      <xr-xml-map>aaa_diameter_base_mib:Diametermib</xr-xml-map>
      <xr-task>aaa</xr-task>
      <protocolerror config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>uint32</xr-primitive-type>
        <xr-path>/cfg/gl/snmp/enable/traps/diameter/ProtoErrorNotif</xr-path>
        <xr-xml-map>aaa_diameter_base_mib:Protocolerror</xr-xml-map>
        <xr-task>aaa</xr-task>
        <xr-sch-type>Boolean</xr-sch-type>
        <type>
          <type-name>boolean</type-name>
        </type>
      </protocolerror>
      <permanentfail config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>uint32</xr-primitive-type>
        <xr-path>/cfg/gl/snmp/enable/traps/diameter/PermanentFailNotif</xr-path>
        <xr-xml-map>aaa_diameter_base_mib:Permanentfail</xr-xml-map>
        <xr-task>aaa</xr-task>
        <xr-sch-type>Boolean</xr-sch-type>
        <type>
          <type-name>boolean</type-name>
        </type>
      </permanentfail>
      <peerdown config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>uint32</xr-primitive-type>
        <xr-path>/cfg/gl/snmp/enable/traps/diameter/PeerDownNotif</xr-path>
        <xr-xml-map>aaa_diameter_base_mib:Peerdown</xr-xml-map>
        <xr-task>aaa</xr-task>
        <xr-sch-type>Boolean</xr-sch-type>
        <type>
          <type-name>boolean</type-name>
        </type>
      </peerdown>
      <peerup config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>uint32</xr-primitive-type>
        <xr-path>/cfg/gl/snmp/enable/traps/diameter/PeerUpNotif</xr-path>

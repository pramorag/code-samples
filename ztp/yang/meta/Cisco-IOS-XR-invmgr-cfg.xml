<?xml version="1.0"?>
<Cisco-IOS-XR-invmgr-cfg ns="http://cisco.com/ns/yang/Cisco-IOS-XR-invmgr-cfg" revision="2015-11-09">
  <files>
    <file revision="2015-11-09" submodule="false">Cisco-IOS-XR-invmgr-cfg.yang</file>
  </files>
  <prefix>invmgr-cfg</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <import prefix="xr">Cisco-IOS-XR-types</import>
  <inventory-configurations config="true" is-key="false" node="container" presence="false">
    <xr-path>/cfg/gl/inventory/</xr-path>
    <xr-xml-map>invmgr_cfg:InventoryConfigurationTable</xr-xml-map>
    <xr-task>inventory</xr-task>
    <entity config="true" key="name" node="list">
      <xr-path>/cfg/gl/inventory/&lt;invmgr_cfg_Entity_name&gt;</xr-path>
      <xr-xml-map>invmgr_cfg:Entity</xr-xml-map>
      <xr-task>inventory</xr-task>
      <name config="true" is-key="true" is-yang-key="true" node="leaf">
        <xr-data-handler>ignore</xr-data-handler>
        <xr-primitive-type>string</xr-primitive-type>
        <xr-path>/cfg/gl/inventory/&lt;invmgr_cfg_Entity_name&gt;</xr-path>
        <xr-xml-map>invmgr_cfg:Name</xr-xml-map>
        <xr-task>inventory</xr-task>
        <xr-sch-type>EncodedString</xr-sch-type>
        <xr-sch-arg-name>&lt;invmgr_cfg_Entity_name&gt;</xr-sch-arg-name>
        <type>
          <type-name>string</type-name>
        </type>
      </name>
      <name-xr config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>string</xr-primitive-type>
        <xr-path>/cfg/gl/inventory/&lt;invmgr_cfg_Entity_name&gt;</xr-path>
        <xr-xml-map>invmgr_cfg:Name</xr-xml-map>
        <xr-task>inventory</xr-task>
        <xr-sch-type>EncodedString</xr-sch-type>
        <type>
          <type-name>string</type-name>
        </type>
        <mandatory>true</mandatory>
      </name-xr>
    </entity>
  </inventory-configurations>
</Cisco-IOS-XR-invmgr-cfg>

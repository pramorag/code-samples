<?xml version="1.0"?>
<Cisco-IOS-XR-config-mda-cfg ns="http://cisco.com/ns/yang/Cisco-IOS-XR-config-mda-cfg" revision="2015-11-09">
  <files>
    <file revision="2015-11-09" submodule="false">Cisco-IOS-XR-config-mda-cfg.yang</file>
  </files>
  <prefix>config-mda-cfg</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <import prefix="xr">Cisco-IOS-XR-types</import>
  <active-nodes config="true" is-key="false" node="container" presence="false">
    <xr-path>/cfg/node/act/</xr-path>
    <xr-xml-map>mda_common_cfg:ActiveNodeTable</xr-xml-map>
    <active-node config="true" key="node-name" node="list">
      <xr-path>/cfg/node/act/&lt;mda_common_cfg_ActiveNode_nodeid&gt;/</xr-path>
      <xr-xml-map>mda_common_cfg:ActiveNode</xr-xml-map>
      <xr-scope>exported</xr-scope>
      <node-name config="true" is-key="true" is-yang-key="true" node="leaf">
        <xr-data-handler>ignore</xr-data-handler>
        <xr-primitive-type>string</xr-primitive-type>
        <xr-path>/cfg/node/act/&lt;mda_common_cfg_ActiveNode_nodeid&gt;/</xr-path>
        <xr-xml-map>mda_common_cfg:NodeName</xr-xml-map>
        <xr-sch-type>StringNodeID</xr-sch-type>
        <xr-sch-arg-name>&lt;mda_common_cfg_ActiveNode_nodeid&gt;</xr-sch-arg-name>
        <type>
          <type-name>xr:Node-id</type-name>
        </type>
      </node-name>
    </active-node>
  </active-nodes>
  <preconfigured-nodes config="true" is-key="false" node="container" presence="false">
    <xr-path>/cfg/node/pre/</xr-path>
    <xr-xml-map>mda_common_cfg:PreconfiguredNodeTable</xr-xml-map>
    <preconfigured-node config="true" key="node-name" node="list">
      <xr-path>/cfg/node/pre/&lt;mda_common_cfg_PreconfiguredNode_nodeid&gt;/</xr-path>
      <xr-xml-map>mda_common_cfg:PreconfiguredNode</xr-xml-map>
      <xr-scope>exported</xr-scope>
      <node-name config="true" is-key="true" is-yang-key="true" node="leaf">
        <xr-data-handler>ignore</xr-data-handler>
        <xr-primitive-type>string</xr-primitive-type>
        <xr-path>/cfg/node/pre/&lt;mda_common_cfg_PreconfiguredNode_nodeid&gt;/</xr-path>
        <xr-xml-map>mda_common_cfg:NodeName</xr-xml-map>
        <xr-sch-type>StringNodeID</xr-sch-type>
        <xr-sch-arg-name>&lt;mda_common_cfg_PreconfiguredNode_nodeid&gt;</xr-sch-arg-name>
        <type>
          <type-name>xr:Node-id</type-name>
        </type>
      </node-name>
    </preconfigured-node>
  </preconfigured-nodes>
</Cisco-IOS-XR-config-mda-cfg>

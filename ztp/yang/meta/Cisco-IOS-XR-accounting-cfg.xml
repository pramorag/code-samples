<?xml version="1.0"?>
<Cisco-IOS-XR-accounting-cfg ns="http://cisco.com/ns/yang/Cisco-IOS-XR-accounting-cfg" revision="2017-05-31">
  <files>
    <file revision="2017-05-31" submodule="false">Cisco-IOS-XR-accounting-cfg.yang</file>
  </files>
  <prefix>accounting-cfg</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <import prefix="xr">Cisco-IOS-XR-types</import>
  <accounting config="true" is-key="false" node="container" presence="false">
    <xr-path>/cfg/gl/accounting/</xr-path>
    <xr-xml-map>accounting_cfg:Accounting</xr-xml-map>
    <xr-task>basic-services</xr-task>
    <interfaces config="true" is-key="false" node="container" presence="false">
      <xr-path>/cfg/gl/accounting/if/</xr-path>
      <xr-xml-map>accounting_cfg:Interfaces</xr-xml-map>
      <xr-task>basic-services</xr-task>
      <mpls config="true" is-key="false" node="container" presence="false">
        <xr-path>/cfg/gl/accounting/if/mpls/</xr-path>
        <xr-xml-map>accounting_cfg:MPLS</xr-xml-map>
        <xr-task>basic-services</xr-task>
        <enable config="true" is-key="false" is-yang-key="false" node="leaf">
          <xr-primitive-type>uint32</xr-primitive-type>
          <xr-path>/cfg/gl/accounting/if/mpls/a</xr-path>
          <xr-xml-map>accounting_cfg:Enable</xr-xml-map>
          <xr-task>basic-services</xr-task>
          <xr-sch-type>Configured</xr-sch-type>
          <type>
            <type-name>empty</type-name>
          </type>
        </enable>
        <enable-v4rsvpte config="true" is-key="false" is-yang-key="false" node="leaf">
          <xr-primitive-type>uint32</xr-primitive-type>
          <xr-path>/cfg/gl/accounting/if/mpls/ipv4-rsvpte</xr-path>
          <xr-xml-map>accounting_cfg:EnableV4RSVPTE</xr-xml-map>
          <xr-task>basic-services</xr-task>
          <xr-sch-type>Configured</xr-sch-type>
          <type>
            <type-name>empty</type-name>
          </type>
        </enable-v4rsvpte>
      </mpls>
      <segment-routing config="true" is-key="false" node="container" presence="false">
        <xr-path>/cfg/gl/accounting/if/sr-mpls/</xr-path>
        <xr-xml-map>accounting_cfg:SegmentRouting</xr-xml-map>
        <xr-task>basic-services</xr-task>
        <enable config="true" is-key="false" is-yang-key="false" node="leaf">
          <xr-primitive-type>uint32</xr-primitive-type>
          <xr-path>/cfg/gl/accounting/if/sr-mpls/a</xr-path>
          <xr-xml-map>accounting_cfg:Enable</xr-xml-map>
          <xr-task>basic-services</xr-task>
          <xr-sch-type>Configured</xr-sch-type>
          <type>
            <type-name>empty</type-name>
          </type>
        </enable>
        <enable-mplsv4 config="true" is-key="false" is-yang-key="false" node="leaf">
          <xr-primitive-type>uint32</xr-primitive-type>
          <xr-path>/cfg/gl/accounting/if/sr-mpls/sr-mpls-v4</xr-path>
          <xr-xml-map>accounting_cfg:EnableMPLSV4</xr-xml-map>
          <xr-task>basic-services</xr-task>
          <xr-sch-type>Configured</xr-sch-type>
          <type>
            <type-name>empty</type-name>
          </type>
        </enable-mplsv4>
        <enable-mplsv6 config="true" is-key="false" is-yang-key="false" node="leaf">
          <xr-primitive-type>uint32</xr-primitive-type>
          <xr-path>/cfg/gl/accounting/if/sr-mpls/sr-mpls-v6</xr-path>
          <xr-xml-map>accounting_cfg:EnableMPLSV6</xr-xml-map>
          <xr-task>basic-services</xr-task>
          <xr-sch-type>Configured</xr-sch-type>
          <type>
            <type-name>empty</type-name>
          </type>
        </enable-mplsv6>
      </segment-routing>
      <enable config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>uint32</xr-primitive-type>
        <xr-path>/cfg/gl/accounting/if/a</xr-path>
        <xr-xml-map>accounting_cfg:Enable</xr-xml-map>
        <xr-task>basic-services</xr-task>
        <xr-sch-type>Configured</xr-sch-type>
        <type>
          <type-name>empty</type-name>
        </type>
      </enable>
    </interfaces>
    <enable config="true" is-key="false" is-yang-key="false" node="leaf">
      <xr-primitive-type>uint32</xr-primitive-type>
      <xr-path>/cfg/gl/accounting/a</xr-path>
      <xr-xml-map>accounting_cfg:Enable</xr-xml-map>
      <xr-task>basic-services</xr-task>
      <xr-sch-type>Configured</xr-sch-type>
      <type>
        <type-name>empty</type-name>
      </type>
    </enable>
  </accounting>
</Cisco-IOS-XR-accounting-cfg>

<?xml version="1.0"?>
<Cisco-IOS-XR-syslog-act ns="http://cisco.com/ns/yang/Cisco-IOS-XR-syslog-act" revision="2016-04-17">
  <files>
    <file revision="2016-04-17" submodule="false">Cisco-IOS-XR-syslog-act.yang</file>
  </files>
  <prefix>syslog-act</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <import prefix="log">ietf-syslog-types</import>
  <logmsg config="true" is-key="false" node="rpc" presence="false">
    <xr-task>universal</xr-task>
    <output config="false" is-key="false" node="output"/>
    <xr-exec>syslog_api &lt;syslog-act:logmsg/severity&gt; &lt;syslog-act:logmsg/message&gt;</xr-exec>
    <input config="false" is-key="false" node="input">
      <severity config="false" is-key="false" is-yang-key="false" node="leaf">
        <xr-task>universal</xr-task>
        <type>
          <type-name>log:severity</type-name>
        </type>
        <mandatory>true</mandatory>
      </severity>
      <message config="false" is-key="false" is-yang-key="false" node="leaf">
        <xr-task>universal</xr-task>
        <type>
          <type-name>string</type-name>
        </type>
        <mandatory>true</mandatory>
      </message>
    </input>
  </logmsg>
</Cisco-IOS-XR-syslog-act>

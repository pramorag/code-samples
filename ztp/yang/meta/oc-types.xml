<?xml version="1.0"?>
<openconfig-types ns="http://openconfig.net/yang/openconfig-types" oc-ver="0.3.2" revision="2017-01-13">
  <files>
    <file revision="2017-01-13" submodule="false">openconfig-types.yang</file>
  </files>
  <prefix>oc-types</prefix>
  <organization>OpenConfig working group</organization>
  <import prefix="oc-ext">openconfig-extensions</import>
  <identity name="ADDRESS_FAMILY"/>
  <identity name="IPV4">
    <base>ADDRESS_FAMILY</base>
  </identity>
  <identity name="IPV6">
    <base>ADDRESS_FAMILY</base>
  </identity>
  <identity name="MPLS">
    <base>ADDRESS_FAMILY</base>
  </identity>
  <identity name="L2_ETHERNET">
    <base>ADDRESS_FAMILY</base>
  </identity>
  <typedef name="percentage">
    <type range="0..100">
      <type-name>uint8</type-name>
    </type>
  </typedef>
  <typedef name="std-regexp">
    <type>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="timeticks64">
    <type>
      <type-name>uint64</type-name>
    </type>
  </typedef>
  <typedef name="ieeefloat32">
    <type length="32">
      <type-name>binary</type-name>
    </type>
  </typedef>
  <typedef name="routing-password">
    <type>
      <type-name>string</type-name>
    </type>
  </typedef>
</openconfig-types>

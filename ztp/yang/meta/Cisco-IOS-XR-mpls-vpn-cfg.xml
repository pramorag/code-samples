<?xml version="1.0"?>
<Cisco-IOS-XR-mpls-vpn-cfg ns="http://cisco.com/ns/yang/Cisco-IOS-XR-mpls-vpn-cfg" revision="2017-09-07">
  <files>
    <file revision="2017-09-07" submodule="false">Cisco-IOS-XR-mpls-vpn-cfg.yang</file>
  </files>
  <prefix>mpls-vpn-cfg</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <import prefix="xr">Cisco-IOS-XR-types</import>
  <import prefix="a1">Cisco-IOS-XR-snmp-agent-cfg</import>
  <augment path="/a1:snmp/a1:notification">
    <mpls-l3vpn config="true" is-key="false" node="container" presence="false">
      <xr-path>/cfg/gl/snmp/enable/traps/l3vpnmib/</xr-path>
      <xr-xml-map>l3vpnmib_cfg:MPLS_L3VPN</xr-xml-map>
      <xr-task>ipv4</xr-task>
      <max-threshold-reissue-notification-time config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>uint32</xr-primitive-type>
        <xr-path>/cfg/gl/snmp/enable/traps/l3vpnmib/notif-reissue-time</xr-path>
        <xr-xml-map>l3vpnmib_cfg:MaxThresholdReissueNotificationTime</xr-xml-map>
        <xr-task>ipv4</xr-task>
        <xr-sch-type>Integer</xr-sch-type>
        <type>
          <type-name>uint32</type-name>
        </type>
      </max-threshold-reissue-notification-time>
      <max-threshold-exceeded config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>uint32</xr-primitive-type>
        <xr-path>/cfg/gl/snmp/enable/traps/l3vpnmib/max-thresh-exc</xr-path>
        <xr-xml-map>l3vpnmib_cfg:MaxThresholdExceeded</xr-xml-map>
        <xr-task>ipv4</xr-task>
        <xr-sch-type>Configured</xr-sch-type>
        <type>
          <type-name>empty</type-name>
        </type>
      </max-threshold-exceeded>
      <max-threshold-cleared config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>uint32</xr-primitive-type>
        <xr-path>/cfg/gl/snmp/enable/traps/l3vpnmib/max-thresh-clr</xr-path>
        <xr-xml-map>l3vpnmib_cfg:MaxThresholdCleared</xr-xml-map>
        <xr-task>ipv4</xr-task>
        <xr-sch-type>Configured</xr-sch-type>
        <type>
          <type-name>empty</type-name>
        </type>
      </max-threshold-cleared>
      <mid-threshold-exceeded config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>uint32</xr-primitive-type>
        <xr-path>/cfg/gl/snmp/enable/traps/l3vpnmib/mid-thresh-exc</xr-path>
        <xr-xml-map>l3vpnmib_cfg:MidThresholdExceeded</xr-xml-map>
        <xr-task>ipv4</xr-task>
        <xr-sch-type>Configured</xr-sch-type>
        <type>
          <type-name>empty</type-name>
        </type>
      </mid-threshold-exceeded>
      <enable config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>uint32</xr-primitive-type>
        <xr-path>/cfg/gl/snmp/enable/traps/l3vpnmib/all</xr-path>
        <xr-xml-map>l3vpnmib_cfg:Enable</xr-xml-map>
        <xr-task>ipv4</xr-task>
        <xr-sch-type>Configured</xr-sch-type>
        <type>
          <type-name>empty</type-name>
        </type>
      </enable>
      <vrf-down config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>uint32</xr-primitive-type>
        <xr-path>/cfg/gl/snmp/enable/traps/l3vpnmib/vrf-down</xr-path>
        <xr-xml-map>l3vpnmib_cfg:VRFDown</xr-xml-map>
        <xr-task>ipv4</xr-task>
        <xr-sch-type>Configured</xr-sch-type>
        <type>
          <type-name>empty</type-name>
        </type>
      </vrf-down>
      <vrf-up config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>uint32</xr-primitive-type>
        <xr-path>/cfg/gl/snmp/enable/traps/l3vpnmib/vrf-up</xr-path>
        <xr-xml-map>l3vpnmib_cfg:VRFUp</xr-xml-map>
        <xr-task>ipv4</xr-task>
        <xr-sch-type>Configured</xr-sch-type>
        <type>
          <type-name>empty</type-name>
        </type>
      </vrf-up>
    </mpls-l3vpn>
  </augment>
</Cisco-IOS-XR-mpls-vpn-cfg>

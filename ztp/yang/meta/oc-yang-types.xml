<?xml version="1.0"?>
<openconfig-yang-types ns="http://openconfig.net/yang/types/yang" oc-ver="0.1.1" revision="2017-04-03">
  <files>
    <file revision="2017-04-03" submodule="false">openconfig-yang-types.yang</file>
  </files>
  <prefix>oc-yang</prefix>
  <organization>OpenConfig working group</organization>
  <import prefix="oc-ext">openconfig-extensions</import>
  <typedef name="dotted-quad">
    <type>
      <pattern>^(([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])\.){3}([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])$</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="hex-string">
    <type>
      <pattern>^[0-9a-fA-F]*$</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="counter32">
    <type>
      <type-name>uint32</type-name>
    </type>
  </typedef>
  <typedef name="counter64">
    <type>
      <type-name>uint64</type-name>
    </type>
  </typedef>
  <typedef name="date-and-time">
    <type>
      <pattern>^[0-9]{4}\-[0-9]{2}\-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}(\.[0-9]+)?Z[+-][0-9]{2}:[0-9]{2}$</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="gauge64">
    <type>
      <type-name>uint64</type-name>
    </type>
  </typedef>
  <typedef name="phys-address">
    <type>
      <pattern>^([0-9a-fA-F]{2}(:[0-9a-fA-F]{2})*)?$</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
  <typedef name="mac-address">
    <type>
      <pattern>^[0-9a-fA-F]{2}(:[0-9a-fA-F]{2}){5}$</pattern>
      <type-name>string</type-name>
    </type>
  </typedef>
</openconfig-yang-types>

<?xml version="1.0"?>
<Cisco-IOS-XR-group-cfg ns="http://cisco.com/ns/yang/Cisco-IOS-XR-group-cfg" revision="2016-04-29">
  <files>
    <file revision="2016-04-29" submodule="false">Cisco-IOS-XR-group-cfg.yang</file>
  </files>
  <prefix>group-cfg</prefix>
  <import prefix="xr">Cisco-IOS-XR-types</import>
  <groups config="true" is-key="false" node="container" presence="false">
    <xr-path>/cfg/gl/a/config-group/</xr-path>
    <xr-task>config-services</xr-task>
    <group config="true" key="group-name" node="list">
      <xr-path>/cfg/gl/a/config-group/&lt;groupname&gt;/</xr-path>
      <xr-task>config-services</xr-task>
      <group-name config="true" is-key="true" node="leaf">
        <xr-data-handler>ignore</xr-data-handler>
        <xr-primitive-type>string</xr-primitive-type>
        <xr-path>/cfg/gl/a/config-group/&lt;groupname&gt;/</xr-path>
        <xr-task>config-services</xr-task>
        <xr-sch-type>BoundedString</xr-sch-type>
        <xr-sch-arg-name>&lt;groupname&gt;</xr-sch-arg-name>
        <type length="0..32">
          <type-name>xr:Cisco-ios-xr-string</type-name>
        </type>
      </group-name>
    </group>
  </groups>
  <apply-groups config="true" is-key="false" node="container" presence="false">
    <xr-path>/ag`</xr-path>
    <xr-task>config-services</xr-task>
    <xr-pack-format>%s%s%s%s%s%s%s%s</xr-pack-format>
    <apply-group config="true" is-key="false" node="leaf">
      <xr-data-handler>preserve-pack</xr-data-handler>
      <xr-is-packed/>
      <xr-pack-format>%s%s%s%s%s%s%s%s</xr-pack-format>
      <xr-pack-elem-size>1</xr-pack-elem-size>
      <xr-path>/ag`0</xr-path>
      <xr-sch-type>String</xr-sch-type>
      <type>
        <type-name>string</type-name>
      </type>
    </apply-group>
  </apply-groups>
</Cisco-IOS-XR-group-cfg>

<?xml version="1.0"?>
<Cisco-IOS-XR-pbr-bng-cfg ns="http://cisco.com/ns/yang/Cisco-IOS-XR-pbr-bng-cfg" revision="2015-11-09">
  <files>
    <file revision="2015-11-09" submodule="false">Cisco-IOS-XR-pbr-bng-cfg.yang</file>
  </files>
  <prefix>pbr-bng-cfg</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <import prefix="xr">Cisco-IOS-XR-types</import>
  <typedef name="Bng-pbr-http-enrichment-params">
    <xr-sch-type>Enum</xr-sch-type>
    <type node="enumeration">
      <enum value="1">subscriber-mac</enum>
      <enum value="2">subscriber-ip</enum>
      <enum value="4">host-name</enum>
      <enum value="8">bng-identifier-interface</enum>
    </type>
  </typedef>
  <bng-pbr config="true" is-key="false" node="container" presence="false">
    <xr-path>/cfg/gl/iedge/pbr/</xr-path>
    <xr-xml-map>pbr_ma_bng:BNG_PBR</xr-xml-map>
    <xr-task>config-services</xr-task>
    <http-enrichment config="true" is-key="false" node="container" presence="false">
      <xr-path>/cfg/gl/iedge/pbr/http-enrichment/</xr-path>
      <xr-xml-map>pbr_ma_bng:Http-enrichment</xr-xml-map>
      <xr-task>config-services</xr-task>
      <parameters config="true" is-key="false" node="container" presence="true">
        <xr-data-handler>ignore</xr-data-handler>
        <xr-is-packed/>
        <xr-path>/cfg/gl/iedge/pbr/http-enrichment/param`</xr-path>
        <xr-xml-map>pbr_ma_bng:Parameters</xr-xml-map>
        <xr-task>config-services</xr-task>
        <xr-pack-format>%d%d%d%d</xr-pack-format>
        <arg1 config="true" is-key="false" is-yang-key="false" node="leaf">
          <xr-is-packed/>
          <xr-pack-format>%d%d%d%d</xr-pack-format>
          <xr-pack-elem-size>1</xr-pack-elem-size>
          <xr-path>/cfg/gl/iedge/pbr/http-enrichment/param`0</xr-path>
          <xr-xml-map>pbr_ma_bng:Arg1</xr-xml-map>
          <xr-task>config-services</xr-task>
          <xr-sch-type>Enum</xr-sch-type>
          <type>
            <type-name>Bng-pbr-http-enrichment-params</type-name>
          </type>
          <mandatory>true</mandatory>
        </arg1>
        <arg2 config="true" is-key="false" is-yang-key="false" node="leaf">
          <xr-is-packed/>
          <xr-pack-format>%d%d%d%d</xr-pack-format>
          <xr-pack-elem-size>1</xr-pack-elem-size>
          <xr-path>/cfg/gl/iedge/pbr/http-enrichment/param`1</xr-path>
          <xr-xml-map>pbr_ma_bng:Arg2</xr-xml-map>
          <xr-task>config-services</xr-task>
          <xr-sch-type>Enum</xr-sch-type>
          <type>
            <type-name>Bng-pbr-http-enrichment-params</type-name>
          </type>
        </arg2>
        <arg3 config="true" is-key="false" is-yang-key="false" node="leaf">
          <xr-is-packed/>
          <xr-pack-format>%d%d%d%d</xr-pack-format>
          <xr-pack-elem-size>1</xr-pack-elem-size>
          <xr-path>/cfg/gl/iedge/pbr/http-enrichment/param`2</xr-path>
          <xr-xml-map>pbr_ma_bng:Arg3</xr-xml-map>
          <xr-task>config-services</xr-task>
          <xr-sch-type>Enum</xr-sch-type>
          <type>
            <type-name>Bng-pbr-http-enrichment-params</type-name>
          </type>
        </arg3>
        <arg4 config="true" is-key="false" is-yang-key="false" node="leaf">
          <xr-is-packed/>
          <xr-pack-format>%d%d%d%d</xr-pack-format>
          <xr-pack-elem-size>1</xr-pack-elem-size>
          <xr-path>/cfg/gl/iedge/pbr/http-enrichment/param`3</xr-path>
          <xr-xml-map>pbr_ma_bng:Arg4</xr-xml-map>
          <xr-task>config-services</xr-task>
          <xr-sch-type>Enum</xr-sch-type>
          <type>
            <type-name>Bng-pbr-http-enrichment-params</type-name>
          </type>
        </arg4>
      </parameters>
    </http-enrichment>
    <bng-interface config="true" is-key="false" is-yang-key="false" node="leaf">
      <xr-primitive-type>string</xr-primitive-type>
      <xr-path>/cfg/gl/iedge/pbr/bng-interface</xr-path>
      <xr-xml-map>pbr_ma_bng:Bng-Interface</xr-xml-map>
      <xr-task>config-services</xr-task>
      <xr-sch-type>InterfaceName</xr-sch-type>
      <type>
        <type-name>xr:Interface-name</type-name>
      </type>
    </bng-interface>
  </bng-pbr>
</Cisco-IOS-XR-pbr-bng-cfg>

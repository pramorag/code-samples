<?xml version="1.0"?>
<Cisco-IOS-XR-ipv4-telnet-cfg ns="http://cisco.com/ns/yang/Cisco-IOS-XR-ipv4-telnet-cfg" revision="2015-11-09">
  <files>
    <file revision="2015-11-09" submodule="false">Cisco-IOS-XR-ipv4-telnet-cfg.yang</file>
  </files>
  <prefix>ipv4-telnet-cfg</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <import prefix="xr">Cisco-IOS-XR-types</import>
  <ipv6-telnet config="true" is-key="false" node="container" presence="false">
    <xr-path>/cfg/gl/ipv6/telnet/</xr-path>
    <xr-xml-map>telnet_client_cfg:IPv6Telnet</xr-xml-map>
    <xr-task>ip-services</xr-task>
    <client config="true" is-key="false" node="container" presence="false">
      <xr-path>/cfg/gl/ipv6/telnet/client/</xr-path>
      <xr-xml-map>telnet_client_cfg:Client</xr-xml-map>
      <xr-task>ip-services</xr-task>
      <source-interface config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>string</xr-primitive-type>
        <xr-path>/cfg/gl/ipv6/telnet/client/source</xr-path>
        <xr-xml-map>telnet_client_cfg:SourceInterface</xr-xml-map>
        <xr-task>ip-services</xr-task>
        <xr-sch-type>InterfaceName</xr-sch-type>
        <type>
          <type-name>xr:Interface-name</type-name>
        </type>
      </source-interface>
    </client>
  </ipv6-telnet>
  <ipv4-telnet config="true" is-key="false" node="container" presence="false">
    <xr-path>/cfg/gl/ipv4/telnet/</xr-path>
    <xr-xml-map>telnet_client_cfg:IPv4Telnet</xr-xml-map>
    <xr-task>ipv4</xr-task>
    <client config="true" is-key="false" node="container" presence="false">
      <xr-path>/cfg/gl/ipv4/telnet/client/</xr-path>
      <xr-xml-map>telnet_client_cfg:Client</xr-xml-map>
      <xr-task>ipv4</xr-task>
      <source-interface config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>string</xr-primitive-type>
        <xr-path>/cfg/gl/ipv4/telnet/client/source</xr-path>
        <xr-xml-map>telnet_client_cfg:SourceInterface</xr-xml-map>
        <xr-task>ipv4</xr-task>
        <xr-sch-type>InterfaceName</xr-sch-type>
        <type>
          <type-name>xr:Interface-name</type-name>
        </type>
      </source-interface>
    </client>
  </ipv4-telnet>
</Cisco-IOS-XR-ipv4-telnet-cfg>

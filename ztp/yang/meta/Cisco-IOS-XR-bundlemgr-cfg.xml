    <type node="enumeration">
      <enum value="1">brute-force</enum>
      <enum value="2">revertive</enum>
    </type>
  </typedef>
  <typedef name="Bundle-load-balance">
    <xr-sch-type>Enum</xr-sch-type>
    <type node="enumeration">
      <enum value="0">default</enum>
      <enum value="1">efp-auto</enum>
      <enum value="2">efp-value</enum>
      <enum value="3">source-ip</enum>
      <enum value="4">destination-ip</enum>
    </type>
  </typedef>
  <typedef name="Bundle-port-activity">
    <xr-sch-type>Enum</xr-sch-type>
    <type node="enumeration">
      <enum value="1">on</enum>
      <enum value="2">active</enum>
      <enum value="3">passive</enum>
      <enum value="4">inherit</enum>
    </type>
  </typedef>
  <typedef name="Mlacp-maximize-parameter">
    <xr-sch-type>Enum</xr-sch-type>
    <type node="enumeration">
      <enum value="1">links</enum>
      <enum value="2">bandwidth</enum>
    </type>
  </typedef>
  <typedef name="Bundle-period">
    <xr-sch-type>RangeEnum</xr-sch-type>
    <type node="union">
      <type node="enumeration">
        <enum value="1">true</enum>
      </type>
      <type range="1..1000">
        <type-name>uint32</type-name>
      </type>
    </type>
  </typedef>
  <augment path="/a1:interface-configurations/a1:interface-configuration">
    <bfd config="true" is-key="false" node="container" presence="false">
      <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bfd/</xr-path>
      <xr-xml-map>bundlemgr_bfd_cfg:BFD</xr-xml-map>
      <xr-task>bundle</xr-task>
      <address-family config="true" is-key="false" node="container" presence="false">
        <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bfd/address-family/</xr-path>
        <xr-xml-map>bundlemgr_bfd_cfg:AddressFamily</xr-xml-map>
        <xr-task>bundle</xr-task>
        <ipv6 config="true" is-key="false" node="container" presence="false">
          <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bfd/address-family/ipv6/</xr-path>
          <xr-xml-map>bundlemgr_bfd_cfg:IPv6</xr-xml-map>
          <xr-task>bundle</xr-task>
          <ipv6-timers config="true" is-key="false" node="container" presence="false">
            <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bfd/address-family/ipv6/timers/</xr-path>
            <xr-xml-map>bundlemgr_bfd_cfg:IPv6Timers</xr-xml-map>
            <xr-task>bundle</xr-task>
            <ipv6-nbor-unconfig-timer config="true" is-key="false" is-yang-key="false" node="leaf">
              <xr-primitive-type>uint32</xr-primitive-type>
              <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bfd/address-family/ipv6/timers/nbr-unconfig</xr-path>
              <xr-xml-map>bundlemgr_bfd_cfg:IPv6NborUnconfigTimer</xr-xml-map>
              <xr-task>bundle</xr-task>
              <xr-sch-type>Range</xr-sch-type>
              <type range="60..3600">
                <type-name>uint32</type-name>
              </type>
            </ipv6-nbor-unconfig-timer>
            <ipv6-start-timer config="true" is-key="false" is-yang-key="false" node="leaf">
              <xr-primitive-type>uint32</xr-primitive-type>
              <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bfd/address-family/ipv6/timers/start</xr-path>
              <xr-xml-map>bundlemgr_bfd_cfg:IPv6StartTimer</xr-xml-map>
              <xr-task>bundle</xr-task>
              <xr-sch-type>Range</xr-sch-type>
              <type range="60..3600">
                <type-name>uint32</type-name>
              </type>
            </ipv6-start-timer>
          </ipv6-timers>
          <ipv6-destination-address config="true" is-key="false" is-yang-key="false" node="leaf">
            <xr-primitive-type>string</xr-primitive-type>
            <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bfd/address-family/ipv6/destination</xr-path>
            <xr-xml-map>bundlemgr_bfd_cfg:IPv6DestinationAddress</xr-xml-map>
            <xr-task>bundle</xr-task>
            <xr-sch-type>String</xr-sch-type>
            <type>
              <type-name>string</type-name>
            </type>
          </ipv6-destination-address>
          <ipv6-detection-multiplier config="true" is-key="false" is-yang-key="false" node="leaf">
            <xr-primitive-type>uint32</xr-primitive-type>
            <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bfd/address-family/ipv6/multiplier</xr-path>
            <xr-xml-map>bundlemgr_bfd_cfg:IPv6DetectionMultiplier</xr-xml-map>
            <xr-task>bundle</xr-task>
            <xr-sch-type>Range</xr-sch-type>
            <type range="2..50">
              <type-name>uint32</type-name>
            </type>
          </ipv6-detection-multiplier>
          <ipv6-fast-detect config="true" is-key="false" is-yang-key="false" node="leaf">
            <xr-primitive-type>uint32</xr-primitive-type>
            <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bfd/address-family/ipv6/fast-detect</xr-path>
            <xr-xml-map>bundlemgr_bfd_cfg:IPv6FastDetect</xr-xml-map>
            <xr-task>bundle</xr-task>
            <xr-sch-type>Configured</xr-sch-type>
            <type>
              <type-name>empty</type-name>
            </type>
          </ipv6-fast-detect>
          <ipv6-interval config="true" is-key="false" is-yang-key="false" node="leaf">
            <xr-primitive-type>uint32</xr-primitive-type>
            <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bfd/address-family/ipv6/min-interval</xr-path>
            <xr-xml-map>bundlemgr_bfd_cfg:IPv6Interval</xr-xml-map>
            <xr-task>bundle</xr-task>
            <xr-sch-type>Range</xr-sch-type>
            <type range="3..30000">
              <type-name>uint32</type-name>
            </type>
          </ipv6-interval>
        </ipv6>
        <ipv4 config="true" is-key="false" node="container" presence="false">
          <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bfd/address-family/ipv4/</xr-path>
          <xr-xml-map>bundlemgr_bfd_cfg:IPv4</xr-xml-map>
          <xr-task>bundle</xr-task>
          <echo config="true" is-key="false" node="container" presence="false">
            <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bfd/address-family/ipv4/echo/</xr-path>
            <xr-xml-map>bundlemgr_bfd_cfg:Echo</xr-xml-map>
            <xr-task>bundle</xr-task>
            <min-interval config="true" is-key="false" is-yang-key="false" node="leaf">
              <xr-primitive-type>uint32</xr-primitive-type>
              <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bfd/address-family/ipv4/echo/min-interval</xr-path>
              <xr-xml-map>bundlemgr_bfd_cfg:MinInterval</xr-xml-map>
              <xr-task>bundle</xr-task>
              <xr-sch-type>Range</xr-sch-type>
              <type range="15..2000">
                <type-name>uint32</type-name>
              </type>
            </min-interval>
          </echo>
          <timers config="true" is-key="false" node="container" presence="false">
            <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bfd/address-family/ipv4/timers/</xr-path>
            <xr-xml-map>bundlemgr_bfd_cfg:Timers</xr-xml-map>
            <xr-task>bundle</xr-task>
            <nbor-unconfig-timer config="true" is-key="false" is-yang-key="false" node="leaf">
              <xr-primitive-type>uint32</xr-primitive-type>
              <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bfd/address-family/ipv4/timers/nbr-unconfig</xr-path>
              <xr-xml-map>bundlemgr_bfd_cfg:NborUnconfigTimer</xr-xml-map>
              <xr-task>bundle</xr-task>
              <xr-sch-type>Range</xr-sch-type>
              <type range="60..3600">
                <type-name>uint32</type-name>
              </type>
            </nbor-unconfig-timer>
            <start-timer config="true" is-key="false" is-yang-key="false" node="leaf">
              <xr-primitive-type>uint32</xr-primitive-type>
              <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bfd/address-family/ipv4/timers/start</xr-path>
              <xr-xml-map>bundlemgr_bfd_cfg:StartTimer</xr-xml-map>
              <xr-task>bundle</xr-task>
              <xr-sch-type>Range</xr-sch-type>
              <type range="60..3600">
                <type-name>uint32</type-name>
              </type>
            </start-timer>
          </timers>
          <interval config="true" is-key="false" is-yang-key="false" node="leaf">
            <xr-primitive-type>uint32</xr-primitive-type>
            <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bfd/address-family/ipv4/min-interval</xr-path>
            <xr-xml-map>bundlemgr_bfd_cfg:Interval</xr-xml-map>
            <xr-task>bundle</xr-task>
            <xr-sch-type>Range</xr-sch-type>
            <type range="3..30000">
              <type-name>uint32</type-name>
            </type>
          </interval>
          <detection-multiplier config="true" is-key="false" is-yang-key="false" node="leaf">
            <xr-primitive-type>uint32</xr-primitive-type>
            <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bfd/address-family/ipv4/multiplier</xr-path>
            <xr-xml-map>bundlemgr_bfd_cfg:DetectionMultiplier</xr-xml-map>
            <xr-task>bundle</xr-task>
            <xr-sch-type>Range</xr-sch-type>
            <type range="2..50">
              <type-name>uint32</type-name>
            </type>
          </detection-multiplier>
          <destination-address config="true" is-key="false" is-yang-key="false" node="leaf">
            <xr-primitive-type>inaddr</xr-primitive-type>
            <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bfd/address-family/ipv4/destination</xr-path>
            <xr-xml-map>bundlemgr_bfd_cfg:DestinationAddress</xr-xml-map>
            <xr-task>bundle</xr-task>
            <xr-sch-type>IPV4Address</xr-sch-type>
            <type>
              <type-name>inet:ipv4-address-no-zone</type-name>
            </type>
          </destination-address>
          <fast-detect config="true" is-key="false" is-yang-key="false" node="leaf">
            <xr-primitive-type>uint32</xr-primitive-type>
            <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bfd/address-family/ipv4/fast-detect</xr-path>
            <xr-xml-map>bundlemgr_bfd_cfg:FastDetect</xr-xml-map>
            <xr-task>bundle</xr-task>
            <xr-sch-type>Configured</xr-sch-type>
            <type>
              <type-name>empty</type-name>
            </type>
          </fast-detect>
        </ipv4>
      </address-family>
      <mode config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>uint32</xr-primitive-type>
        <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bfd/mode</xr-path>
        <xr-xml-map>bundlemgr_bfd_cfg:Mode</xr-xml-map>
        <xr-task>bundle</xr-task>
        <xr-sch-type>Enum</xr-sch-type>
        <type>
          <type-name>Bfd-mode</type-name>
        </type>
        <default>no-cfg</default>
      </mode>
    </bfd>
  </augment>
  <augment path="/a1:interface-configurations/a1:interface-configuration">
    <bundle config="true" is-key="false" node="container" presence="false">
      <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bundle/</xr-path>
      <xr-xml-map>bundlemgr_interface_cfg:Bundle</xr-xml-map>
      <xr-task>bundle</xr-task>
      <bundle-load-balancing config="true" is-key="false" node="container" presence="false">
        <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bundle/load-balancing/</xr-path>
        <xr-xml-map>bundlemgr_interface_cfg:BundleLoadBalancing</xr-xml-map>
        <xr-task>bundle</xr-task>
        <xr-scope>exported</xr-scope>
        <hash-function config="true" is-key="false" node="container" presence="true">
          <xr-data-handler>ignore</xr-data-handler>
          <xr-is-packed/>
          <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bundle/load-balancing/hash`</xr-path>
          <xr-xml-map>bundlemgr_interface_cfg:HashFunction</xr-xml-map>
          <xr-task>bundle</xr-task>
          <xr-pack-format>%d%d</xr-pack-format>
          <hash-type config="true" is-key="false" is-yang-key="false" node="leaf">
            <xr-is-packed/>
            <xr-pack-format>%d%d</xr-pack-format>
            <xr-pack-elem-size>1</xr-pack-elem-size>
            <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bundle/load-balancing/hash`0</xr-path>
            <xr-xml-map>bundlemgr_interface_cfg:HashType</xr-xml-map>
            <xr-task>bundle</xr-task>
            <xr-sch-type>Enum</xr-sch-type>
            <type>
              <type-name>Bundle-load-balance</type-name>
            </type>
            <mandatory>true</mandatory>
          </hash-type>
          <hash-value config="true" is-key="false" is-yang-key="false" node="leaf">
            <xr-is-packed/>
            <xr-pack-format>%d%d</xr-pack-format>
            <xr-pack-elem-size>1</xr-pack-elem-size>
            <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bundle/load-balancing/hash`1</xr-path>
            <xr-xml-map>bundlemgr_interface_cfg:HashValue</xr-xml-map>
            <xr-task>bundle</xr-task>
            <xr-sch-type>Range</xr-sch-type>
            <type range="1..64">
              <type-name>uint32</type-name>
            </type>
            <mandatory>true</mandatory>
          </hash-value>
        </hash-function>
        <localize-links config="true" is-key="false" is-yang-key="false" node="leaf">
          <xr-primitive-type>uint32</xr-primitive-type>
          <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bundle/load-balancing/locality-threshold</xr-path>
          <xr-xml-map>bundlemgr_interface_lb_locality_cfg:LocalizeLinks</xr-xml-map>
          <xr-task>bundle</xr-task>
          <xr-sch-type>Range</xr-sch-type>
          <type range="1..64">
            <type-name>uint32</type-name>
          </type>
        </localize-links>
      </bundle-load-balancing>
      <minimum-active config="true" is-key="false" node="container" presence="false">
        <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bundle/minimum_active/</xr-path>
        <xr-xml-map>bundlemgr_interface_cfg:MinimumActive</xr-xml-map>
        <xr-task>bundle</xr-task>
        <bandwidth config="true" is-key="false" node="container" presence="true">
          <xr-data-handler>ignore</xr-data-handler>
          <xr-is-packed/>
          <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bundle/minimum_active/bandwidth`</xr-path>
          <xr-xml-map>bundlemgr_interface_cfg:Bandwidth</xr-xml-map>
          <xr-task>bundle</xr-task>
          <xr-pack-format>%d%d</xr-pack-format>
          <min-bandwidth-range config="true" is-key="false" is-yang-key="false" node="leaf">
            <xr-is-packed/>
            <xr-pack-format>%d%d</xr-pack-format>
            <xr-pack-elem-size>1</xr-pack-elem-size>
            <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bundle/minimum_active/bandwidth`0</xr-path>
            <xr-xml-map>bundlemgr_interface_cfg:MinBandwidthRange</xr-xml-map>
            <xr-task>bundle</xr-task>
            <xr-sch-type>Enum</xr-sch-type>
            <type>
              <type-name>Bundle-minimum-bandwidth-range</type-name>
            </type>
            <mandatory>true</mandatory>
          </min-bandwidth-range>
          <bandwidth config="true" is-key="false" is-yang-key="false" node="leaf">
            <xr-is-packed/>
            <xr-pack-format>%d%d</xr-pack-format>
            <xr-pack-elem-size>1</xr-pack-elem-size>
            <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bundle/minimum_active/bandwidth`1</xr-path>
            <xr-xml-map>bundlemgr_interface_cfg:Bandwidth</xr-xml-map>
            <xr-task>bundle</xr-task>
            <xr-sch-type>Range</xr-sch-type>
            <type range="1..4294967295">
              <type-name>uint32</type-name>
            </type>
            <mandatory>true</mandatory>
          </bandwidth>
        </bandwidth>
        <links config="true" is-key="false" is-yang-key="false" node="leaf">
          <xr-primitive-type>uint32</xr-primitive-type>
          <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/bundle/minimum_active/links</xr-path>
          <xr-xml-map>bundlemgr_interface_cfg:Links</xr-xml-map>
          <xr-task>bundle</xr-task>
          <xr-sch-type>Range</xr-sch-type>
          <type range="1..64">
            <type-name>uint32</type-name>
          </type>
          <default>1</default>
        </links>
      </minimum-active>

<?xml version="1.0"?>
<Cisco-IOS-XR-bundlemgr-cfg ns="http://cisco.com/ns/yang/Cisco-IOS-XR-bundlemgr-cfg" revision="2017-05-01">
  <files>
    <file revision="2017-05-01" submodule="false">Cisco-IOS-XR-bundlemgr-cfg.yang</file>

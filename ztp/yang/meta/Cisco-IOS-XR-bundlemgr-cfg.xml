  <typedef name="Bfd-mode">
    <xr-sch-type>Enum</xr-sch-type>
    <type node="enumeration">
      <enum value="0">no-cfg</enum>
      <enum value="1">cisco</enum>
      <enum value="2">ietf</enum>

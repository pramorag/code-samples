<?xml version="1.0"?>
<Cisco-IOS-XR-clns-isis-datatypes ns="http://cisco.com/ns/yang/Cisco-IOS-XR-clns-isis-datatypes" revision="2017-05-01">
  <files>
    <file revision="2017-05-01" submodule="false">Cisco-IOS-XR-clns-isis-datatypes.yang</file>
  </files>
  <prefix>clns-isis-datatypes</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <import prefix="xr">Cisco-IOS-XR-types</import>
  <typedef name="Isis-address-family">
    <xr-sch-type>Enum</xr-sch-type>
    <type node="enumeration">
      <enum value="0">ipv4</enum>
      <enum value="1">ipv6</enum>
    </type>
  </typedef>
  <typedef name="Isis-ipv6-prefix-length">
    <xr-sch-type>ZeroRange</xr-sch-type>
    <type range="0..128">
      <type-name>uint32</type-name>
    </type>
  </typedef>
  <typedef name="Isis-ipv4-prefix-length">
    <xr-sch-type>ZeroRange</xr-sch-type>
    <type range="0..32">
      <type-name>uint32</type-name>
    </type>
  </typedef>
  <typedef name="Isis-instance-name">
    <xr-sch-type>BoundedString</xr-sch-type>
    <type length="1..36">
      <type-name>xr:Cisco-ios-xr-string</type-name>
    </type>
  </typedef>
  <typedef name="Isis-internal-level">
    <xr-sch-type>Enum</xr-sch-type>
    <type node="enumeration">
      <enum value="0">not-set</enum>
      <enum value="1">level1</enum>
      <enum value="2">level2</enum>
    </type>
  </typedef>
  <typedef name="Isis-link-group-name">
    <xr-sch-type>BoundedString</xr-sch-type>
    <type length="1..40">
      <type-name>xr:Cisco-ios-xr-string</type-name>
    </type>
  </typedef>
  <typedef name="Isis-sub-address-family">
    <xr-sch-type>Enum</xr-sch-type>
    <type node="enumeration">
      <enum value="0">unicast</enum>
      <enum value="1">multicast</enum>
    </type>
  </typedef>
  <typedef name="Isis-topology-name">
    <xr-sch-type>BoundedString</xr-sch-type>
    <type length="1..32">
      <type-name>xr:Cisco-ios-xr-string</type-name>
    </type>
  </typedef>
</Cisco-IOS-XR-clns-isis-datatypes>

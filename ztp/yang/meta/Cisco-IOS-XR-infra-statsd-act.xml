<?xml version="1.0"?>
<Cisco-IOS-XR-infra-statsd-act ns="http://cisco.com/ns/yang/Cisco-IOS-XR-infra-statsd-act" revision="2018-01-10">
  <files>
    <file revision="2018-01-10" submodule="false">Cisco-IOS-XR-infra-statsd-act.yang</file>
  </files>
  <prefix>statsd-act</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <import prefix="csc">Cisco-IOS-XR-types</import>
  <clear-counters-controller config="true" is-key="false" node="rpc" presence="false">
    <xr-task>interface</xr-task>
    <xr-cli-command>clear counters &lt;controller-name&gt;</xr-cli-command>
    <output config="false" is-key="false" node="output"/>
    <input config="false" is-key="false" node="input">
      <controller-name config="false" is-key="false" is-yang-key="false" node="leaf">
        <xr-task>interface</xr-task>
        <mandatory>true</mandatory>
        <type>
          <type-name>csc:Interface-name</type-name>
        </type>
      </controller-name>
    </input>
  </clear-counters-controller>
  <clear-counters-all config="false" is-key="false" node="rpc" presence="false">
    <xr-task>interface</xr-task>
    <xr-cli-command>clear counters all</xr-cli-command>
    <input config="false" is-key="false" node="input"/>
    <output config="false" is-key="false" node="output"/>
  </clear-counters-all>
  <clear-counters-interface config="false" is-key="false" node="rpc" presence="false">
    <xr-task>interface</xr-task>
    <xr-cli-command>clear counters interface &lt;interface-name&gt;</xr-cli-command>
    <output config="false" is-key="false" node="output"/>
    <input config="false" is-key="false" node="input">
      <interface-name config="false" is-key="false" is-yang-key="false" node="leaf">
        <xr-task>interface</xr-task>
        <mandatory>true</mandatory>
        <type>
          <type-name>csc:Interface-name</type-name>
        </type>
      </interface-name>
    </input>
  </clear-counters-interface>
</Cisco-IOS-XR-infra-statsd-act>

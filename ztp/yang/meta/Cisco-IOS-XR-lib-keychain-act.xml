<?xml version="1.0"?>
<Cisco-IOS-XR-lib-keychain-act ns="http://cisco.com/ns/yang/Cisco-IOS-XR-lib-keychain-act" revision="2017-04-17">
  <files>
    <file revision="2017-04-17" submodule="false">Cisco-IOS-XR-lib-keychain-act.yang</file>
  </files>
  <prefix>lib-keychain-act</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <master-key-add config="true" is-key="false" node="rpc" presence="false">
    <xr-task>root-system</xr-task>
    <output config="false" is-key="false" node="output"/>
    <input config="false" is-key="false" node="input">
      <new-key config="false" is-key="false" is-yang-key="false" node="leaf">
        <xr-task>root-system</xr-task>
        <type>
          <type-name>string</type-name>
        </type>
      </new-key>
    </input>
  </master-key-add>
  <master-key-delete config="false" is-key="false" node="rpc" presence="false">
    <xr-task>root-system</xr-task>
    <input config="false" is-key="false" node="input"/>
    <output config="false" is-key="false" node="output"/>
  </master-key-delete>
  <master-key-update config="false" is-key="false" node="rpc" presence="false">
    <xr-task>root-system</xr-task>
    <output config="false" is-key="false" node="output"/>
    <input config="false" is-key="false" node="input">
      <old-key config="false" is-key="false" is-yang-key="false" node="leaf">
        <xr-task>root-system</xr-task>
        <type>
          <type-name>string</type-name>
        </type>
        <mandatory>true</mandatory>
      </old-key>
      <new-key config="false" is-key="false" is-yang-key="false" node="leaf">
        <xr-task>root-system</xr-task>
        <type>
          <type-name>string</type-name>
        </type>
        <mandatory>true</mandatory>
      </new-key>
    </input>
  </master-key-update>
</Cisco-IOS-XR-lib-keychain-act>

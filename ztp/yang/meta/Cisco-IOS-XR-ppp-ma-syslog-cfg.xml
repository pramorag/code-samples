<?xml version="1.0"?>
<Cisco-IOS-XR-ppp-ma-syslog-cfg ns="http://cisco.com/ns/yang/Cisco-IOS-XR-ppp-ma-syslog-cfg" revision="2015-11-09">
  <files>
    <file revision="2015-11-09" submodule="false">Cisco-IOS-XR-ppp-ma-syslog-cfg.yang</file>
  </files>
  <prefix>ppp-ma-syslog-cfg</prefix>
  <organization>Cisco Systems, Inc.</organization>
  <import prefix="xr">Cisco-IOS-XR-types</import>
  <ppp config="true" is-key="false" node="container" presence="false">
    <xr-path>/cfg/gl/ppp/</xr-path>
    <xr-xml-map>ppp_syslog_cfg:PPP</xr-xml-map>
    <xr-task>ppp</xr-task>
    <syslog config="true" is-key="false" node="container" presence="false">
      <xr-path>/cfg/gl/ppp/syslog/</xr-path>
      <xr-xml-map>ppp_syslog_cfg:Syslog</xr-xml-map>
      <xr-task>ppp</xr-task>
      <enable-session-status config="true" is-key="false" is-yang-key="false" node="leaf">
        <xr-primitive-type>uint32</xr-primitive-type>
        <xr-path>/cfg/gl/ppp/syslog/session-status-enable</xr-path>
        <xr-xml-map>ppp_syslog_cfg:EnableSessionStatus</xr-xml-map>
        <xr-task>ppp</xr-task>
        <xr-sch-type>Configured</xr-sch-type>
        <type>
          <type-name>empty</type-name>
        </type>
      </enable-session-status>
    </syslog>
  </ppp>
</Cisco-IOS-XR-ppp-ma-syslog-cfg>

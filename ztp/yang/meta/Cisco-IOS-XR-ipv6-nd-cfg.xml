          </dns-search-list>
          <lifetime config="true" is-key="false" is-yang-key="false" node="leaf">
            <xr-primitive-type>uint32</xr-primitive-type>
            <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/ipv6/nd/ra-dns-search-list/&lt;ipv6_nd_interface_cfg_RADNSSearchList_dns_search_list&gt;</xr-path>

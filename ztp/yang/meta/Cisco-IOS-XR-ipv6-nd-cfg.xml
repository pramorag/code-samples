  <import prefix="xr">Cisco-IOS-XR-types</import>
  <import prefix="a1">Cisco-IOS-XR-ifmgr-cfg</import>
  <typedef name="Ipv6srp-encapsulation">
    <xr-sch-type>Enum</xr-sch-type>
    <type node="enumeration">
      <enum value="5">srpa</enum>
      <enum value="6">srpb</enum>
    </type>
  </typedef>
  <typedef name="Ipv6nd-month">
    <xr-sch-type>Enum</xr-sch-type>
    <type node="enumeration">
      <enum value="0">january</enum>
      <enum value="1">february</enum>
      <enum value="2">march</enum>
      <enum value="3">april</enum>
      <enum value="4">may</enum>
      <enum value="5">june</enum>
      <enum value="6">july</enum>
      <enum value="7">august</enum>
      <enum value="8">september</enum>
      <enum value="9">october</enum>
      <enum value="10">november</enum>
      <enum value="11">december</enum>
    </type>
  </typedef>
  <typedef name="Ipv6-nd-router-pref">

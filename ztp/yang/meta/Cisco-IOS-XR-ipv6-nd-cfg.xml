      <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/ipv6/nd/</xr-path>
      <xr-xml-map>ipv6_nd_interface_cfg:IPv6Neighbor</xr-xml-map>
      <xr-task>network &amp; ipv6</xr-task>
      <raspecific-route-s config="true" is-key="false" node="container" presence="false">
        <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/ipv6/nd/ra-specific-route/</xr-path>
        <xr-xml-map>ipv6_nd_interface_cfg:RASPECIFICROUTETable</xr-xml-map>
        <xr-task>network &amp; ipv6</xr-task>
        <raspecific-route config="true" key="specific-route" node="list">
          <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/ipv6/nd/ra-specific-route/&lt;ipv6_nd_interface_cfg_RASPECIFICROUTE_specific_route&gt;</xr-path>
          <xr-xml-map>ipv6_nd_interface_cfg:RASPECIFICROUTE</xr-xml-map>
          <xr-task>network &amp; ipv6</xr-task>
          <xr-pack-format>%d%d%d</xr-pack-format>
          <specific-route config="true" is-key="true" is-yang-key="true" node="leaf">
            <xr-is-packed/>
            <xr-pack-format>%d%d%d</xr-pack-format>
            <xr-primitive-type>string</xr-primitive-type>
            <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/ipv6/nd/ra-specific-route/&lt;ipv6_nd_interface_cfg_RASPECIFICROUTE_specific_route&gt;</xr-path>
            <xr-xml-map>ipv6_nd_interface_cfg:SpecificRoute</xr-xml-map>
            <xr-task>network &amp; ipv6</xr-task>
            <xr-sch-type>IPV6AddressString</xr-sch-type>
            <xr-sch-arg-name>&lt;ipv6_nd_interface_cfg_RASPECIFICROUTE_specific_route&gt;</xr-sch-arg-name>
            <type>
              <type-name>inet:ipv6-address-no-zone</type-name>
            </type>
          </specific-route>
          <prefix-length config="true" is-key="false" is-yang-key="false" node="leaf">
            <xr-data-handler>preserve-pack</xr-data-handler>
            <xr-is-packed/>
            <xr-pack-format>%d%d%d</xr-pack-format>
            <xr-pack-elem-size>1</xr-pack-elem-size>
            <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/ipv6/nd/ra-specific-route/&lt;ipv6_nd_interface_cfg_RASPECIFICROUTE_specific_route&gt;`0</xr-path>

            <xr-task>network &amp; ipv6</xr-task>
            <xr-sch-type>IPV6PrefixLength</xr-sch-type>
            <type>
              <type-name>xr:Ipv6-prefix-length</type-name>
            </type>
          </prefix-length>
          <lifetime config="true" is-key="false" is-yang-key="false" node="leaf">
            <xr-data-handler>preserve-pack</xr-data-handler>
            <xr-is-packed/>
            <xr-pack-format>%d%d%d</xr-pack-format>
            <xr-pack-elem-size>1</xr-pack-elem-size>
            <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/ipv6/nd/ra-specific-route/&lt;ipv6_nd_interface_cfg_RASPECIFICROUTE_specific_route&gt;`1</xr-path>
            <xr-xml-map>ipv6_nd_interface_cfg:Lifetime</xr-xml-map>
            <xr-task>network &amp; ipv6</xr-task>
            <xr-sch-type>ZeroRange</xr-sch-type>
            <type range="0..4294967295">
              <type-name>uint32</type-name>
            </type>
          </lifetime>
          <preference config="true" is-key="false" is-yang-key="false" node="leaf">
            <xr-data-handler>preserve-pack</xr-data-handler>
            <xr-is-packed/>
            <xr-pack-format>%d%d%d</xr-pack-format>
            <xr-pack-elem-size>1</xr-pack-elem-size>
            <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/ipv6/nd/ra-specific-route/&lt;ipv6_nd_interface_cfg_RASPECIFICROUTE_specific_route&gt;`2</xr-path>
            <xr-xml-map>ipv6_nd_interface_cfg:Preference</xr-xml-map>
            <xr-task>network &amp; ipv6</xr-task>
            <xr-sch-type>Enum</xr-sch-type>
            <type>
              <type-name>Ipv6-nd-router-pref</type-name>
            </type>
          </preference>
        </raspecific-route>
      </raspecific-route-s>
      <radns-searches config="true" is-key="false" node="container" presence="false">
        <xr-path>/cfg/if/&lt;ifmgr_cfg_InterfaceConfiguration_active&gt;/&lt;ifmgr_cfg_InterfaceConfiguration_name&gt;/ipv6/nd/ra-dns-search-list/</xr-path>
